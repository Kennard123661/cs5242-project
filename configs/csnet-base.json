  "model": "ir-csn",
  "optimizer": "sgd",
  "scheduler": "half-cosine",
  "dataset": "breakfast",
  "warmup-scheduler": 1,

  "clip-length": 32,
  "lr": 0.01,
  "weight-decay": 0,
  "max-epochs": 10,
  "train-batch-size": 4,
  "test-batch-size": 20,

  "lr-decay-step-size": 100,
  "lr-decay-rate": 0.5
}
